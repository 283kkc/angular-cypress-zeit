{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    {
      "src": "index.js",
      "use": "@now/node",
      "config": {
        "includeFiles": [
          "dist/angular-tour-of-heroes/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
